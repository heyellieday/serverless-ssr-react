{
  "name": "my-express-application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "transpile": "babel --presets=env,react src --out-dir bin",
    "predeploy": "npm run transpile && npm run bundle",
    "deploy": "node ./deploy.js",
    "postdeploy": "rm -rf bin",
    "predeploy:function": "npm run transpile",
    "deploy:function": "sls deploy function -f app",
    "postdeploy:function": "rm -rf bin",
    "logs": "serverless logs -f app -t",
    "bundle": "webpack",
    "start": "concurrently --kill-others \"webpack-dev-server\" \"nodemon index.js --exec babel-node --presets=env,react\""
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTesting.js"
  },
