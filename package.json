{
  "name": "my-express-application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "babel --presets=env,react src --out-dir bin",
    "predeploy": "npm run transpile",
    "deploy": "serverless deploy -v",
    "postdeploy": "rm -rf bin",
    "predeploy:function": "npm run transpile",
    "deploy:function": "sls deploy function -f app",
    "postdeploy:function": "rm -rf bin",
    "logs": "serverless logs -f app -t",
    "bundle": "webpack",
    "start":
      "concurrently --kill-others \"webpack-dev-server\" \"nodemon index.js --exec babel-node --presets=env,react\""
  },
